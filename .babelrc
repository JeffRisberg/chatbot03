{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-object-rest-spread",
        [
            "module-resolver", {
            "root": "./app/js",
            "alias": {
                "Events": "js/components/Events",
                "Items": "js/components/Items",
                "pages": "js/pages"
            }
        }
        ]
    ]
}

